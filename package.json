{
    "name": "flutter-typer",
    "displayName": "flutter-typer",
    "description": "Type in flutter code",
    "version": "0.0.1",
    "publisher": "brettmorgan",
    "license": "Apache-2.0",
    "repository": {
        "type": "git",
        "url": "sso://user/brettmorgan/devfest-flutter-typer"
    },
    "engines": {
        "vscode": "^1.31.0"
    },
    "categories": [
        "Other"
    ],
    "activationEvents": [
        "onCommand:extension.devFestNext",
        "onCommand:extension.devFestPrevious",
        "onCommand:extension.devFestResetMain"
    ],
    "main": "./out/extension",
    "contributes": {
        "commands": [
            {
                "command": "extension.devFestResetMain",
                "title": "Reset Main",
                "category": "DevFest"
            }
        ],
        "keybindings": [
            {
                "command": "extension.devFestNext",
                "key": "ctrl+right",
                "mac": "cmd+right",
                "when": "editorTextFocus"
            },
            {
                "command": "extension.devFestPrevious",
                "key": "ctrl+left",
                "mac": "cmd+left",
                "when": "editorTextFocus"
            }
        ]
    },
    "scripts": {
        "vscode:prepublish": "npm run compile",
        "compile": "tsc -p ./",
        "watch": "tsc -watch -p ./",
        "postinstall": "node ./node_modules/vscode/bin/install",
        "test": "npm run compile && node ./node_modules/vscode/bin/test",
        "prettier": "prettier --single-quote --trailing-comma=all --arrow-parens=always --write src/*.ts",
        "npm-update": "ncu -u"
    },
    "devDependencies": {
        "@types/diff": "^4.0.1",
        "@types/mocha": "^5.2.5",
        "@types/node": "^11.9.5",
        "diff": "^4.0.1",
        "npm-check-updates": "^2.15.0",
        "prettier": "1.16.4",
        "tslint": "^5.13.0",
        "typescript": "^3.2.2",
        "vscode": "^1.1.30"
    },
    "dependencies": {
        "jsonc-parser": "^2.0.3",
        "vscode-uri": "^1.0.6"
    }
}
